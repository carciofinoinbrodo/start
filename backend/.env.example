# CORS - comma-separated list of allowed frontend origins
CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# Database (SQLite is default, uncomment for PostgreSQL)
# DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/aiseo

# Browser settings (for scraper)
BROWSER_HEADLESS=false
BROWSER_SLOW_MO_MS=50

# Rate limits (requests per hour)
CHATGPT_RATE_LIMIT=20
PERPLEXITY_RATE_LIMIT=30
GOOGLE_AI_RATE_LIMIT=50

# Proxy settings (optional)
# PROXY_PROVIDER_API_KEY=your_api_key
# PROXY_PROVIDER_ENDPOINT=https://proxy.example.com

# Logging
LOG_LEVEL=INFO
DEBUG=false
